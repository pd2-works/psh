<?xml version="1.0" encoding="utf-8" ?>

<manifest>

    <function item="string">
        <parameter name="versionCode">115</parameter>
    </function>

    <function item="safeCheck">
        <parameter name="verify">@file/sign.ver</parameter>
        <parameter name="minLibVersion">115</parameter>
    </function>

    <function item="i18n">
        <parameter name="directory">@file/lang</parameter>
    </function>

    <plugin name="Pd2 Shell"
            versionCode="@string/versionCode"
            mainClass="ink.pd2.shell.buildin.CorePlugin"/>

    <extension name="Pd2 Shell Linux Platform Extension"
               mainClass="ink.maxelbk.psh.LinuxPlatformExtension"/>

</manifest>
